	<!-- topbar ends -->
		<div class="container-fluid">
		<div class="row-fluid">
			<!-- left menu starts -->
			<div class="span2 main-menu-span">
				<div class="well nav-collapse sidebar-nav">
                @if (User.Identity.IsAuthenticated) { 
                @*    <input type="hidden" id="userId" value="@Membership.GetUser().ProviderUserKey"/>*@                   
					@Html.Action("UserMenu", "Index", new  { Area = "AdminPanel"})
					<label id="for-is-ajax" class="hidden-tablet" for="is-ajax"><input id="is-ajax" type="checkbox"> Динамическая загрузка страниц</label>
                }
				</div><!--/.well -->
                
			</div><!--/span-->
			<!-- left menu ends -->
			
			<noscript>
				<div class="alert alert-block span10">
					<h4 class="alert-heading">Извините!</h4>
					<p>Для работы на сайте необходимо включить <a href="http://en.wikipedia.org/wiki/JavaScript" target="_blank">JavaScript</a>. Спасибо.</p>
				</div>
			</noscript>
			
			<div id="content" class="span10">
			<!-- content starts -->
			@RenderBody()<!-- content ends -->
			</div><!--/#content.span10-->
				</div><!--/fluid-row-->
				
		<hr>

		<div class="modal hide fade" id="myModal">
			<div class="modal-header">
				<button type="button" class="close" data-dismiss="modal">×</button>
				<h3>Пожалуйста, подождите. Идет загрузка формы...</h3>
			</div>
			<div class="modal-body">
				<img src="/AdminPanel/img/ajax-loaders/ajax-loader-6.gif" title="/AdminPanel/img/ajax-loaders/ajax-loader-6.gif">
			</div>
			<div class="modal-footer">
				<a href="#" class="btn" data-dismiss="modal">Отменить загрузку формы</a>
			</div>
		</div>

		<footer>
			<p class="pull-left">&nbsp;&copy;&nbsp;<a href="https://www.facebook.com/alex.godance" target="_blank">AlexGoDance</a> @DateTime.Now.Year</p>
			<p class="pull-right">Дизайн и верстка: <a href="http://usman.it/free-responsive-admin-template">Charisma</a></p>
		</footer>
		
