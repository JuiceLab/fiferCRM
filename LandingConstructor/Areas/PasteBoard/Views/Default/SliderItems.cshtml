@model IList<ISliderItem>
           @if(Model.Count > 0)
           { 
<tr>
    <td height="350" style="background-color: White;">

        <div id="wowslider-container10">
            <div class="ws_images">
                
                <div style="width: 100%; visibility: hidden; font-size: 0px; line-height: 0;">
                    <img src="@Model[0].PhotoPath" alt="@Model[0].Title" title="@Model[0].Title" style="width: 100%;"></div>

                <ul style="position: absolute; top: 0px; -webkit-animation: none; width: 1000%; left: 0%;">
                    @foreach (var item in Model.OrderBy(m => m.Pos))
                    {
                        <li style="width: 10%; font-size: 0px;"><a href="@item.RedirectAnchor" target="@(item.IsBlank?"_blank":"")"><img src="@item.PhotoPath" alt="@item.Title" title="@item.Title" style="visibility: hidden;"></a></li>
                    }
                </ul>
            </div>
        </div>
    </td>
</tr>
           }