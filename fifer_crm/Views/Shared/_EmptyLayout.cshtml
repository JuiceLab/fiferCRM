            '/assets/library/bootstrap/js/bootstrap.min.js?v=v2.0.0-rc1&sv=v0.0.1.2',
            '/assets/library/jquery/jquery-migrate.min.js?v=v2.0.0-rc1&sv=v0.0.1.2',
        ],

        /* PLUGINS always load after CORE and PLUGINS_DEPENDENCY, but before the BUNDLE / initialization scripts; */
        plugins: [
                    '/assets/plugins/core_preload/pace.min.js?v=v2.0.0-rc1&sv=v0.0.1.2',
                    '/assets/plugins/core_nicescroll/jquery.nicescroll.min.js?v=v2.0.0-rc1&sv=v0.0.1.2',
                    '/assets/plugins/core_breakpoints/breakpoints.js?v=v2.0.0-rc1&sv=v0.0.1.2',
                    '/assets/plugins/forms_wizards/jquery.bootstrap.wizard.js?v=v2.0.0-rc1&sv=v0.0.1.2',
