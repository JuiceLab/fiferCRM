                    '/assets/plugins/core_less-js/less.min.js?v=v2.0.0-rc1&sv=v0.0.1.2',
                    '/assets/plugins/charts_flot/excanvas.js?v=v2.0.0-rc1&sv=v0.0.1.2',
                    '/assets/plugins/core_browser/ie/ie.prototype.polyfill.js?v=v2.0.0-rc1&sv=v0.0.1.2',
                    '/assets/plugins/forms_elements_inputmask/jquery.inputmask.bundle.min.js?v=v2.0.0-rc1&sv=v0.0.1.2',
                ],

                /* The initialization scripts always load last and are automatically and dynamically loaded when AJAX navigation is enabled; */
                bundle: [
                    '/assets/components/core_preload/preload.pace.init.js?v=v2.0.0-rc1&sv=v0.0.1.2',
                    '/assets/components/forms_elements_fuelux-checkbox/fuelux-checkbox.init.js?v=v2.0.0-rc1&sv=v0.0.1.2',
                    '/assets/components/forms_wizards/form-wizards.init.js?v=v2.0.0-rc1&sv=v0.0.1.2',
                    '/assets/components/forms_elements_multiselect/multiselect.init.js?v=v2.0.0-rc1&sv=v0.0.1.2',
                    '/assets/components/admin_menus/sidebar.main.init.js?v=v2.0.0-rc1',
                    '/assets/components/admin_menus/sidebar.discover.init.js?v=v2.0.0-rc1',
                    '/assets/components/admin_menus/sidebar.kis.init.js?v=v2.0.0-rc1',
                    '/assets/components/core/core.init.js?v=v2.0.0-rc1',
                  
