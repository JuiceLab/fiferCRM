        <script src="/assets/plugins/core_ajaxify_loadscript/script.min.js?v=v2.0.0-rc1&amp;sv=v0.0.1.2"></script>
        <script data-id="App.Scripts">
    var App = {};
    App.Scripts = {

        /* CORE scripts always load first; */
        core: [
            '/assets/library/jquery/jquery.min.js?v=v2.0.0-rc1&sv=v0.0.1.2',
            '/assets/library/modernizr/modernizr.js?v=v2.0.0-rc1&sv=v0.0.1.2'
        ],

        /* PLUGINS_DEPENDENCY always load after CORE but before PLUGINS; */
