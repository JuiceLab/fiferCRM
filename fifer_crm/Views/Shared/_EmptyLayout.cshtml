                    '/scripts/custom/helper.signup.js?v=v2.4.1-rc1&sv=v0.0.1.2'
                ]

            };
        </script>

        <script>
            $script(App.Scripts.core, 'core');

            $script.ready(['core'], function () {
                $script(App.Scripts.plugins_dependency, 'plugins_dependency');
            });
            $script.ready(['core', 'plugins_dependency'], function () {
                $script(App.Scripts.plugins, 'plugins');
            });
            $script.ready(['core', 'plugins_dependency', 'plugins'], function () {
                $script(App.Scripts.bundle, 'bundle');
            });
            $script.ready(['core', 'plugins_dependency', 'bundle'], function () {
                $script(App.Scripts.custom, 'custom');
            });
            
        </script>
        <script data-id="App.Config">
           var basePath = '',
                commonPath = '/assets/',
                rootPath = '/',
                DEV = false,
                componentsPath = '/assets/components/';

            var primaryColor = '#87c844',
                dangerColor = '#b55151',
                successColor = '#609450',
                infoColor = '#4a8bc2',
                warningColor = '#ab7a4b',
                inverseColor = '#45484d';

            var themerPrimaryColor = primaryColor;
            App.Config = {
                ajaxify_menu_selectors: ['#menu', '#menu_kis'],
                ajaxify_layout_app: true
            };
        </script>
